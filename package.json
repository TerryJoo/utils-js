{
  "name": "utils-js",
  "version": "1.0.0",
  "description": "js utils",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerryJoo/utils-js.git"
  },
  "keywords": [
    "ts",
    "js",
    "utils",
    "lib",
    "contrib"
  ],
  "author": "jooyungik@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TerryJoo/utils-js/issues"
  },
  "homepage": "https://github.com/TerryJoo/utils-js#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
    "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
